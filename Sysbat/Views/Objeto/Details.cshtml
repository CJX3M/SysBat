@model SysbatLib.Models.Objeto

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_DetailsLayout.cshtml";
}

    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">Detalles del Objeto</h4>
    </div>
    <div class="modal-body">
        <div class="form-group">
            <div class="input-group">
                @Html.LabelFor(model => model.Nombre, new { @class = "input-group-addon" })
                @Html.TextBoxFor(model => model.Nombre, new { @class = "form-control", @disabled = true })
            </div>
            <div class="input-group">
                @Html.LabelFor(model => model.FechaCreacion, new { @class = "input-group-addon" })
                @Html.TextBoxFor(model => model.FechaCreacion, new { @class = "form-control", @disabled = true })
            </div>
        </div>
    </div>
    <div class="modal-footer">
        @Html.ActionLink("Editar", "Edit", new { id = Model.Oid }, new { @class = "btn btn-info" })
    </div>
